   <children>
      <ImageView fitHeight="427.0" fitWidth="619.0" layoutX="-11.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Assest/Image/login2.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtUserName" layoutX="187.0" layoutY="188.0" prefHeight="40.0" prefWidth="238.0" promptText="Your Name" />
      <Button fx:id="btnLogin" layoutX="275.0" layoutY="250.0" mnemonicParsing="false" onAction="#LogInAction" text="Log In" />
      <ImageView fitHeight="96.0" fitWidth="104.0" layoutX="254.0" layoutY="70.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Assest/Icon/User_Icon.png" />
